{
  "private": true,
  "main": "server.js",
  "workspaces": [
    "react-portfolio",
    "server"
  ],
  "scripts": {
    "start": "npm-run-all --parallel start:react-portfolio start:server",
    "start:react-portfolio": "npm --prefix react-portfolio run dev",
    "start:server": "npm --prefix server run dev",

     "build": "npm-run-all --parallel build:react-portfolio build:server",
    "build:react-portfolio":"npm --prefix react-portfolio run build",
    "build:server":"npm --prefix server run build"
  },
  "dependencies": {
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
